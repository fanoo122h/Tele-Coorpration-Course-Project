<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.laware.Friends_list"
    android:background="@drawable/ran">

    <AutoCompleteTextView
        android:id="@+id/search_friend"
        android:layout_width="296dp"
        android:layout_height="38dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:defaultFocusHighlightEnabled="true"
        android:elevation="16dp"
        android:ems="10"
        android:fadingEdge="horizontal|vertical"
        android:fontFamily="casual"
        android:hapticFeedbackEnabled="true"

        android:hint="Search Friend"
        android:textColor="@android:color/holo_red_light"
        android:textSize="12sp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_editor_absoluteY="5dp"
        tools:ignore="HardcodedText,RtlHardcoded" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="87dp"
        android:layout_height="32dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:fontFamily="casual"
        android:text="Pending"
        android:textAllCaps="true"
        android:textColor="@android:color/holo_red_light"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/search_friend"
        tools:ignore="HardcodedText" />
    <Button
        android:id="@+id/profilebutton"
        android:layout_width="140dp"
        android:layout_height="48dp"
        android:layout_marginBottom="-1dp"
        android:layout_marginRight="-13dp"
        android:background="@android:color/holo_red_light"
        android:elevation="0dp"
        android:fontFamily="casual"
        android:paddingHorizontal="0dp"
        android:paddingVertical="0dp"
        android:text="Profile"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="HardcodedText,RtlHardcoded,UnusedAttribute" />

    <Button
        android:id="@+id/friends"
        android:layout_width="132dp"
        android:layout_height="48dp"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="0dp"
        android:background="@android:color/holo_red_light"
        android:elevation="0dp"
        android:fontFamily="casual"
        android:paddingHorizontal="0dp"
        android:paddingVertical="0dp"
        android:text="Friends"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/profilebutton"
        tools:ignore="HardcodedText,RtlHardcoded,UnusedAttribute" />

    <Button
        android:id="@+id/search_button2"
        android:layout_width="140dp"
        android:layout_height="48dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginStart="2dp"
        android:background="@android:color/holo_red_light"
        android:elevation="0dp"
        android:fontFamily="casual"
        android:paddingHorizontal="0dp"
        android:paddingVertical="0dp"
        android:text="Search"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/friends"
        tools:ignore="HardcodedText,RtlHardcoded,UnusedAttribute" />
    <Button
        android:id="@+id/button_go"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@android:drawable/ic_menu_search"
        android:backgroundTint="@android:color/holo_red_light"
        app:layout_constraintBottom_toTopOf="@+id/ListView"
        app:layout_constraintHorizontal_bias="0.878"
        app:layout_constraintLeft_toRightOf="@+id/search_friend"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button6"
        android:layout_width="147dp"
        android:layout_height="44dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/holo_red_light"
        android:fontFamily="casual"
        android:text="My Friends"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/ListView"
        app:layout_constraintHorizontal_bias="0.944"
        app:layout_constraintLeft_toRightOf="@+id/textView15"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/search_friend"
        app:layout_constraintVertical_bias="0.521"
        tools:ignore="HardcodedText" />

    <ListView
        android:id="@+id/ListView"
        android:layout_width="406dp"
        android:layout_height="502dp"
        android:layout_marginTop="0dp"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />


</android.support.constraint.ConstraintLayout>
